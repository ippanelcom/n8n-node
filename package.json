{
  "name": "n8n-nodes-ippanel",
  "version": "0.2.0",
  "description": "n8n node for IPPanel SMS service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node",
    "ippanel",
    "sms"
  ],
  "author": "IPPanel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ippanelcom/n8n-node.git"
  },
  "bugs": {
    "url": "https://github.com/ippanelcom/n8n-node/issues"
  },
  "homepage": "https://github.com/ippanelcom/n8n-node#readme",
  "dependencies": {
    "axios": "^1.9.0",
    "ippanel-node-sdk": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "eslint": "^8.38.0",
    "prettier": "^2.8.7",
    "typescript": "~5.0.4",
    "n8n-workflow": "^1.82.0",
    "n8n-core": "^1.0.0"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "nodes/IPPanel/IPPanel.node.js"
    ],
    "credentials": [
      "credentials/IPPanelApi.credentials.js"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
